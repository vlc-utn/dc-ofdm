create_clock -period 30.000 -name tck -waveform {0.000 15.000} [get_pins -filter { NAME =~  "*/TCK*" } -of_objects [get_cells -hierarchical -filter { PRIMITIVE_TYPE =~ *.BSCANE2 }]]set_clock_groups -asynchronous -group [get_clocks {tck}] -group [get_clocks -of_objects [get_nets -filter { NAME =~  "*IPCORE_CLK" } -of_objects [get_cells -hierarchical -filter { REF_NAME =~  "*FPGADataCapture" }]]]